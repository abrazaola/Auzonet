<?xml version='1.0' encoding='utf-8'?>
<widget id="com.aitorbrazaola.auzonet" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Notification">
        <param name="ios-package" value="CDVNotification" />
    </feature>
    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
    </feature>
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Compass">
        <param name="ios-package" value="CDVCompass" />
    </feature>
    <name>Auzonet</name>
    <icon src="generic-icon.png" />
    <description>
        Social networking app for neighbors.
    </description>
    <author email="aitor.bv@deusto.es" href="http://deusto.es">
        Aitor Brazaola Vicario
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="http://apps.morelab.deusto.es/auzonet/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon height="180" src="www/img/iPhone/Icon-60@3x.png" width="180" />
    <icon height="60" src="www/img/iPhone/Icon-60.png" width="60" />
    <icon height="120" src="www/img/iPhone/Icon-60@2x.png" width="120" />
    <icon height="76" src="www/img/iPad/Icon-76.png" width="76" />
    <icon height="152" src="www/img/iPad/Icon-76@2x.png" width="152" />
    <icon height="40" src="www/img/iPhone/Icon-Small-40.png" width="40" />
    <icon height="80" src="www/img/iPhone/Icon-Small-40@2x.png" width="80" />
    <icon height="57" src="www/img/iPhone/Icon.png" width="57" />
    <icon height="114" src="www/img/iPhone/Icon@2x.png" width="114" />
    <icon height="72" src="www/img/iPad/Icon-72.png" width="72" />
    <icon height="144" src="www/img/iPad/Icon-72@2x.png" width="144" />
    <icon height="29" src="www/img/iPhone/Icon-Small.png" width="29" />
    <icon height="58" src="www/img/iPhone/Icon-Small@2x.png" width="58" />
    <icon height="50" src="www/img/iPad/Icon-Small-50.png" width="50" />
    <icon height="100" src="www/img/iPad/Icon-Small-50@2x.png" width="100" />
    <splash height="480" src="www/img/Default~iphone.png" width="320" />
    <splash height="960" src="www/img/Default@2x~iphone.png" width="640" />
    <splash height="1136" src="www/img/Default-568h@2x~iphone.png" width="640" />
    <splash height="1334" src="www/img/portrait-splash@2x.png" width="750" />
    <splash height="2208" src="www/img/portrait-splash@3x.png" width="1242" />
    <splash height="750" src="www/img/landscape-splash-@2x.png" width="1334" />
    <splash height="1242" src="www/img/landscape-splash-@3x.png" width="2208" />
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="SuppressesLongPressGesture" value="false" />
    <preference name="Suppresses3DTouchGesture" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <preference name="StatusBarBackgroundColor" value="#000000" />
</widget>
